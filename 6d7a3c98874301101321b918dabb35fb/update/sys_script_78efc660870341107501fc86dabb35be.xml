        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var assigned_cases = new GlideRecord('x_snc_avail_dash_assigned_case');
	assigned_cases.addEncodedQuery('assignee=' + current.assignee);
	assigned_cases.orderByDesc('assigned_on');
	assigned_cases.query();

	if(assigned_cases.next()){

		var assignee = new GlideRecord('x_snc_avail_dash_assignee');
		assignee.get(current.assignee);
		assignee.last_assigned = assigned_cases.assigned_on;
		assignee.update();

	}

})(current, previous);]]></script>
