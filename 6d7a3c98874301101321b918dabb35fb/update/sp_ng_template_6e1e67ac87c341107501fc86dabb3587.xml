        <template><![CDATA[<div>
  <div class="modal-header">
    <h4 class="modal-title">
      ${Add Assignee}
    </h4>
  </div>

  <div class="modal-body">

    <div class = "form-group">
      <label class = "field-label" for = "name">Name</label>
      <input class = "form-control" id = "name" ng-model = "assignee_request.user_name"/>
    </div>

    <div class = "form-group">
      <label class = "field-label" for = "region">Region</label>
      <select class = "form-control" id = "region" ng-model = "assignee_request.region">
        <option value = ''>-- None --</option>
        <option ng-repeat = "region in data.region_array" value = "{{region}}">{{region}}</option>
      </select>
    </div>

    <div class = "form-group">
      <label class = "field-label" for = "email">Email</label>
      <input class = "form-control" id = "email" ng-model = "assignee_request.email"/>
    </div>

    <div class = "form-group">
      <label class = "field-label" for = "name">Phone (please use E.164 format. Ex: +12345678910)</label>
      <input class = "form-control" id = "name" ng-model = "assignee_request.phone"/>
    </div>


    <div class = "form-group">
      <label class = "field-label" for = "skills">Skills</label>
      <textarea class = "form-control" id = "skills" ng-model = "assignee_request.skills">
      </textarea>
    </div>
  </div>

  <div class = "modal-footer">
    <button class = "btn btn-primary" ng-click = "submitRequest(assignee_request)" ng-disabled = "!assignee_request.user_name || !assignee_request.region || !assignee_request.email || !assignee_request.phone || !assignee_request.skills">
      ${Submit}
    </button>
  </div>
</div>]]></template>
    </sp_ng_template>
</record_update>
