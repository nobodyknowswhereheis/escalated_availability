      <sp-date-picker id = "assigned_on" field="new_case_request.assigned_on" ng-model="new_case_request.assigned_on" sn-include-time="true"></sp-date-picker>
    </div>
  </div>
  
  <div class = "modal-footer">
    <button class = "btn btn-primary" ng-click = "submitRequest(new_case_request)" ng-disabled = "!new_case_request.case_number || !new_case_request.assigned_on">
      ${Submit}
    </button>
  </div>
</div>]]></template>
